{
  "manifest_version": 2,
  "content_scripts": [
    {
      "exclude_globs": [],
      "include_globs": [
        "*"
      ],
      "js": [
        "inject.js"
      ],
      "matches": [
        "https://*.slack.com/messages/*",
        "http://*.slack.com/messages/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "index.js",
    "promise-queue.js"
  ],
  "converted_from_user_script": true,
  "description": "Use Control+Shift+Space delete the message under the cursor.",
  "name": "Slack Easy Delete",
  "version": "5.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
